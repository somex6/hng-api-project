      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 21
      },
      "id": 4,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "expr": "sum(rate({container_name=~\"$container\"}[5m])) by (container_name)",
          "legendFormat": "{{container_name}}",
          "refId": "A"
        }
      ],
      "title": "Log Volume by Container",
      "type": "timeseries"
    }
